{
  "logDir": "logs",
  "appenders": {
    "console": { "enabled": true },
    "file": {
      "enabled": true,
      "filename": "collector.log",
      "maxLogSize": 10485760,
      "backups": 3,
      "compress": true
    }
  },
  "categories": {
    "default": { "level": "debug" },
    "app": { "level": "debug" },
    "db": { "level": "warn" },
    "binance": { "level": "debug" },
    "gate": { "level": "debug" },
    "bitget": { "level": "debug" }
  }
}

